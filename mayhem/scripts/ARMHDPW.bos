#define TA			// This is a TA script

#include "sfxtype.h"
#include "exptype.h"
//#include "unitsweaps.h"

piece  rflare, lflare, torso, ruparm, luparm, rloarm, lloarm, pelvis,
       rthigh, lthigh, lleg, rleg, rfoot, lfoot, ground, rguncap,
       lguncap, llaser, rlaser, head;

static-var  bMoving, bAiming, gun_1, Waterdepth_4, Walking_5;

Walk()
{
	Walking_5 = 1;
	while( bMoving )
	{
		if( bMoving )
		{
			move pelvis to y-axis [-0.540] now;
			turn rthigh to x-axis <10.692> speed <480>;
			turn lthigh to x-axis <-53.489> speed <480>;
			if( !bAiming )
			{
				turn torso to y-axis <5.747> speed <480>;
				turn luparm to x-axis <38.670> speed <480>;
				turn ruparm to x-axis <-18.093> speed <480>;
				turn rloarm to x-axis <48.956> speed <480>;
				turn lloarm to x-axis <48.956> speed <480>;
			}
			turn lleg to x-axis <53.489> speed <480>;
			turn rleg to x-axis <53.489> speed <480>;
			turn rfoot to x-axis <-37.846> speed <480>;
			sleep 90;
