			turn rthigh to x-axis <-10.280> speed <480>;
			turn lthigh to x-axis <-41.549> speed <480>;
			if( !bAiming )
			{
				turn torso to y-axis <3.280> speed <480>;
				turn luparm to x-axis <32.093> speed <480>;
