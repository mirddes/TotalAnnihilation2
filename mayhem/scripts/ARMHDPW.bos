			turn rleg to x-axis <81.879> speed <480>;
			turn rfoot to x-axis <-31.258> speed <480>;
			sleep 70;
			move pelvis to y-axis [-0.150] now;
			turn rthigh to x-axis <-18.093> speed <480>;
			turn lthigh to x-axis <-24.681> speed <480>;
			if( !bAiming )
			{
				turn torso to y-axis <0> speed <480>;
				turn luparm to x-axis <27.148> speed <480>;
				turn ruparm to x-axis <-7.401> speed <480>;
			}
			turn lleg to x-axis <24.681> speed <480>;
			turn rleg to x-axis <85.582> speed <480>;
			turn rfoot to x-axis <-23.027> speed <480>;
			if( Waterdepth_4 == 3 )
			{
				emit-sfx 259 from lfoot;
			}
			sleep 50;
		}
		if( bMoving )
		{
			move pelvis to y-axis [0] now;
			turn rthigh to x-axis <-42.379> speed <480>;
			turn lthigh to x-axis <5.335> speed <480>;
			if( !bAiming )
			{
				turn torso to y-axis <-3.280> speed <480>;
				turn luparm to x-axis <8.214> speed <480>;
				turn ruparm to x-axis <9.027> speed <480>;
				turn rloarm to x-axis <48.956> speed <480>;
				turn lloarm to x-axis <48.956> speed <480>;
			}
			turn lleg to x-axis <6.571> speed <480>;
			turn lfoot to x-axis <-9.027> speed <480>;
			turn rleg to x-axis <104.516> speed <480>;
			turn rfoot to x-axis <0> speed <480>;
			sleep 40;
			move pelvis to y-axis [-0.200] now;
			turn rthigh to x-axis <-69.945> speed <480>;
			turn lthigh to x-axis <14.802> speed <480>;
			if( !bAiming )
			{
				turn torso to y-axis <-6.159> speed <480>;
				turn luparm to x-axis <-3.692> speed <480>;
				turn ruparm to x-axis <17.681> speed <480>;
			}
			turn lleg to x-axis <15.214> speed <480>;
			turn lfoot to x-axis <-25.093> speed <480>;
			turn rleg to x-axis <107.401> speed <480>;
			sleep 50;
			move pelvis to y-axis [-0.400] now;
			turn rthigh to x-axis <-75.291> speed <480>;
			turn lthigh to x-axis <20.137> speed <480>;
			if( !bAiming )
			{
				turn torso to y-axis <-8.214> speed <480>;
				turn luparm to x-axis <-10.280> speed <480>;
				turn ruparm to x-axis <25.093> speed <480>;
			}
			turn lleg to x-axis <26.324> speed <480>;
			turn lfoot to x-axis <-33.725> speed <480>;
			turn rleg to x-axis <64.604> speed <480>;
			if( Waterdepth_4 == 3 )
			{
				emit-sfx 259 from rfoot;
			}
			sleep 70;
		}
		if( bMoving )
		{
			move pelvis to y-axis [-0.550] now;
			turn rthigh to x-axis <-56.780> speed <480>;
			turn lthigh to x-axis <24.258> speed <480>;
			if( !bAiming )
			{
				turn torso to y-axis <-6.159> speed <480>;
				turn luparm to x-axis <-23.857> speed <480>;
				turn ruparm to x-axis <31.258> speed <480>;
				turn rloarm to x-axis <48.956> speed <480>;
				turn lloarm to x-axis <48.956> speed <480>;
			}
			turn lleg to x-axis <38.670> speed <480>;
			turn lfoot to x-axis <-38.670> speed <480>;
			turn rleg to x-axis <56.780> speed <480>;
			sleep 90;
			move pelvis to y-axis [-0.300] now;
			turn rthigh to x-axis <-44.846> speed <480>;
			turn lthigh to x-axis <5.335> speed <480>;
			if( !bAiming )
			{
				turn torso to y-axis <-3.280> speed <480>;
				turn luparm to x-axis <-16.038> speed <480>;
				turn ruparm to x-axis <19.725> speed <480>;
			}
			turn lleg to x-axis <67.071> speed <480>;
			turn rfoot to x-axis <-12.324> speed <480>;
			sleep 70;
			move pelvis to y-axis [-0.140] now;
			turn rthigh to x-axis <-22.615> speed <480>;
			turn lthigh to x-axis <-12.736> speed <480>;
			if( !bAiming )
			{
				turn torso to y-axis <0> speed <480>;
				turn luparm to x-axis <-1.637> speed <480>;
				turn ruparm to x-axis <10.280> speed <480>;
			}
			turn lleg to x-axis <79.412> speed <480>;
			turn lfoot to x-axis <-27.967> speed <480>;
			turn rleg to x-axis <37.027> speed <480>;
			turn rfoot to x-axis <-15.626> speed <480>;
			if( Waterdepth_4 == 3 )
			{
				emit-sfx 259 from lfoot;
			}
			sleep 50;
		}
		if( bMoving )
		{
			move pelvis to y-axis [0] now;
			turn rthigh to x-axis <2.868> speed <480>;
			turn lthigh to x-axis <-42.379> speed <480>;
			if( !bAiming )
