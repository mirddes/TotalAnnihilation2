RestoreAfterDelay()
{
	timer_6 = 0;
	sleep 3000;
	if( !timer_6 )
	{
		signal 2;
		signal 4;
		signal 8;
		set-signal-mask 0;
		turn torso to y-axis <0> speed <90>;
		turn head to y-axis <0> speed <150>;
		turn head to x-axis <0> speed <70>;
		turn luparm to z-axis <0> speed <50>;
		turn ruparm to z-axis <0> speed <50>;
		wait-for-turn torso around y-axis;
		if( !timer_6 )
		{
			aiming_4 = 0;
			turn rmissle2 to z-axis <0> speed <60>;
			turn lmissle2 to z-axis <0> speed <60>;
			move rmissle2 to y-axis [0] speed [10];
			move lmissle2 to y-axis [0] speed [10];
			if( !moving_3 )
			{
				turn luparm to x-axis <0> speed <120>;
				turn ruparm to x-axis <0> speed <120>;
			}
		}
	}
	Return (0);
}

SmokeUnit(healthpercent, sleeptime, smoketype)
{
	while( get BUILD_PERCENT_LEFT )
	{
		sleep 400;
	}
	/*set GRANT_UNITINFO to 1;
	get SET_UNITINFO(uiHealTime,20);*/
	while( TRUE )
	{
		healthpercent = get HEALTH;
		if( healthpercent < 66 )
		{
			smoketype = 256 | 2;
			if( Rand( 1, 66 ) < healthpercent )
			{
				smoketype = 256 | 1;
			}
			if( terrain_5 <= 3 )
			{
				smoketype = 256 | 3;
			}
			emit-sfx smoketype from torso;
		}
		sleeptime = healthpercent * 50;
		if( sleeptime < 200 )
		{
			sleeptime = 200;
		}
		sleep sleeptime;
		if( timer_6 > 0 )
		{
			timer_6 = timer_6 - sleeptime;
			if( timer_6 < 1 )
			{
				start-script RestoreAfterDelay();
			}
		}
	}
}

Create()
{
	set ARMORED to 1;
	hide eyeflare;
	hide lgunflare;
	hide rgunflare;
	dont-cache eyegun;
	dont-shade eyegun;
	gun_1 = 0;
	gun_2 = 0;
	moving_3 = 0;
	aiming_4 = 0;
	timer_6 = 0;
	busy_7 = 0;
	reload_8 = 0;
	while( get BUILD_PERCENT_LEFT )
	{
		sleep 400;
	}
	start-script SmokeUnit();
}

