}

FireTertiary()
{
	if( !gun_1 )
	{
		turn ruparm to x-axis <83> speed <200>;
		turn rarm to x-axis <-98> speed <200>;
		show rgunflare;
		sleep 150;
		hide rgunflare;
		turn rarm to x-axis <-84> speed <100>;
		turn ruparm to x-axis <0> speed <100>;
		gun_1 = 1;
