AimPrimary(heading, pitch)
{
	signal 4;
	set-signal-mask 4;
	turn head to y-axis heading speed <300>;
	if( pitch > 0 )
	{
		turn head to x-axis 0 - pitch speed <160>;
	}
	if( pitch <= 0 )
	{
		turn head to x-axis 0 speed <160>;
	}
	while( reload_8 )
	{
		wait-for-move eyeflare along z-axis;
	}
	wait-for-turn head around y-axis;
	wait-for-turn head around x-axis;
	timer_6 = 10000;
	return (1);
}

AimSecondary(heading, pitch)
{
	move rmissle2 to y-axis [6] speed [15];
	move lmissle2 to y-axis [6] speed [15];
	turn rmissle2 to z-axis <-13> speed <60>;
	turn lmissle2 to z-axis <13> speed <60>;
	signal 8;
	set-signal-mask 8;
	wait-for-move rmissle2 along y-axis;
	wait-for-move lmissle2 along y-axis;
	timer_6 = 10000;
	return (1);
}

AimTertiary(heading, pitch)
{
	if( !aiming_4 )
	{
		aiming_4 = 1;
		sleep 300;
	}
	signal 2;
	set-signal-mask 2;
	move rmissle2 to y-axis [6] speed [10];
	move lmissle2 to y-axis [6] speed [10];
	turn rmissle2 to z-axis <-13> speed <60>;
	turn lmissle2 to z-axis <13> speed <60>;
	turn luparm to x-axis <0> speed <120>;
	turn ruparm to x-axis <0> speed <120>;
	turn luparm to z-axis <-45> speed <80>;
	turn ruparm to z-axis <45> speed <80>;
	turn rarm to x-axis <-84> speed <120>;
	turn larm to x-axis <-84> speed <120>;
	turn torso to y-axis heading speed <100>;
	wait-for-turn luparm around z-axis;
	wait-for-turn ruparm around z-axis;
	wait-for-turn luparm around x-axis;
	wait-for-turn ruparm around x-axis;
	wait-for-turn rarm around x-axis;
	wait-for-turn larm around x-axis;
	wait-for-turn torso around y-axis;
	timer_6 = 10000;
	return (1);
}

FirePrimary()
{
	reload_8 = 1;
	show eyeflare;
	move eyeflare to z-axis [0.305] speed [100];
	sleep 150;
	hide eyeflare;
	move eyeflare to z-axis [0] speed [0.1];
	wait-for-move eyeflare along z-axis;
	reload_8 = 0;
	Return (0);
}

